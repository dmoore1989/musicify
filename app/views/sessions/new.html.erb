<h1>Log In</h1>

<form action="<%= session_url %>" method="post">
  <%= auth_token %>
  <label for "user_email">EMAIL ADDRESS:</label>
  <input type="string" name="user[:email]" id="user_email" value="<%= @user.email %>">
  <br>
  <label for "user_password">PASSWORD:</label>
  <input type="string" name="user[:password]" id="user_password" value="<%= @user.password %>">
  <br>
  <input type="submit" value="Log In!"
</form>
